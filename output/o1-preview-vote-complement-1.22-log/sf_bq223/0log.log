LEFT JOIN CPC_Titles
    ON CPC_Titles."symbol" = Citations_To_Denso."CPC_Main_Code"
WHERE CPC_Titles."titleFull" IS NOT NULL
GROUP BY "Assignee", "CPC_Subclass_Title"
ORDER BY "Citation_Count" DESC
LIMIT 100;
