  GROUP BY
    c.state_code,
    state_name,
    c.inventory_year
),
average_macroplot_sizes AS (
  SELECT
    c.state_code,
    c.state_code_name AS state_name,
    c.inventory_year,
    AVG(c.condition_proportion_unadjusted * p.expansion_factor * p.adjustment_factor_for_the_macroplot) AS avg_size
  FROM
