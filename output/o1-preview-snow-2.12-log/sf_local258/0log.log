        SUM(s."runs_scored") AS "runs_conceded_in_match"
    FROM "IPL"."IPL"."BALL_BY_BALL" b
    LEFT JOIN "IPL"."IPL"."WICKET_TAKEN" w
        ON b."match_id" = w."match_id"
        AND b."over_id" = w."over_id"
        AND b."ball_id" = w."ball_id"
