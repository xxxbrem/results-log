These queries help in understanding the distribution of data across the relevant tables and prepare us to compute the average subplot and macroplot sizes per state and year as required.Query:
WITH plot_data AS (
  SELECT
    c.state_code_name AS State,
    c.inventory_year AS Year,
    c.proportion_basis AS Proportion_Basis,
    CASE
      WHEN c.proportion_basis = 'SUBP' THEN 'subplot'
      WHEN c.proportion_basis = 'MACR' THEN 'macroplot'
    END AS Type_of_Plot,
    c.condition_status_code,
    c.condition_proportion_unadjusted AS P,
    p.expansion_factor AS E,
    p.adjustment_factor_for_the_subplot AS A_s,
    p.adjustment_factor_for_the_macroplot AS A_m
  FROM
    `bigquery-public-data.usfs_fia.condition` AS c
  JOIN
    `bigquery-public-data.usfs_fia.population` AS p
  ON
    c.plot_sequence_number = p.plot_sequence_number
  WHERE
    c.condition_status_code = 1  -- Accessible forest land
    AND p.evaluation_type = 'EXPALL'  -- Adjusted based on available data
    AND c.inventory_year IN (2015, 2016, 2017)
),

plot_areas AS (
  SELECT
    Type_of_Plot,
    Year,
    State,
    CASE
      WHEN Type_of_Plot = 'subplot' AND A_s > 0 THEN E * P * A_s
      WHEN Type_of_Plot = 'macroplot' AND A_m > 0 THEN E * P * A_m
      ELSE 0
    END AS Plot_Area
  FROM
    plot_data
  WHERE
    (Type_of_Plot = 'subplot' AND A_s > 0)
    OR
    (Type_of_Plot = 'macroplot' AND A_m > 0)
),

average_plot_size AS (
  SELECT
    Type_of_Plot,
    Year,
    State,
    AVG(Plot_Area) AS Average_Size
  FROM
    plot_areas
  GROUP BY
    Type_of_Plot, Year, State
),

ranked_plot_size AS (
  SELECT
    Type_of_Plot,
    Year,
    State,
    Average_Size,
    ROW_NUMBER() OVER(PARTITION BY Type_of_Plot, Year ORDER BY Average_Size DESC) AS rn
  FROM
    average_plot_size
)

SELECT
  Type_of_Plot,
  Year,
  State,
  Average_Size
FROM
  ranked_plot_size
WHERE
  rn = 1
ORDER BY
  Type_of_Plot, Year;
