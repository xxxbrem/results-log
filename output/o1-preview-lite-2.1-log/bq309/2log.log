JOIN
  `bigquery-public-data.stackoverflow.users` AS u
ON
  q.owner_user_id = u.id
LEFT JOIN (
  SELECT
    user_id,
    COUNT(*) AS Badge_Count
  FROM
    `bigquery-public-data.stackoverflow.badges`
  GROUP BY
    user_id
