    SELECT
      1
    FROM
      `bigquery-public-data.stackoverflow.posts_answers` AS a
    WHERE
      a.parent_id = q.id
      AND (a.score / NULLIF(q.view_count, 0)) > 0.01
  )
ORDER BY
  LENGTH(q.body) DESC
LIMIT 10;
