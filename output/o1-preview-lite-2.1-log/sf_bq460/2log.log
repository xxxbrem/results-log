        COALESCE(ARRAY_AGG('%' || word || '%'), ARRAY_CONSTRUCT('')) AS patterns
    FROM (
        SELECT DISTINCT w.value::STRING AS word
        FROM WORD_VECTORS_US.WORD_VECTORS_US.NATURE,
