SELECT n."id", n."date", n."title"
FROM WORD_VECTORS_US.WORD_VECTORS_US.NATURE n, target_patterns
WHERE EXISTS (
    SELECT 1
    FROM TABLE(FLATTEN(input => target_patterns.patterns)) AS p
    WHERE n."title" ILIKE p.value
)
LIMIT 100;
