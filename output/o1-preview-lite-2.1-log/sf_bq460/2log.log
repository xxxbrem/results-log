    FROM (
        SELECT DISTINCT w.value::STRING AS word
        FROM WORD_VECTORS_US.WORD_VECTORS_US.NATURE n,
             LATERAL FLATTEN(input => SPLIT(LOWER(REGEXP_REPLACE(n."title", '[^\\w\\s]+', '')), ' ')) w
