Query:
SELECT
  c.plot_sequence_number,
  c.state_code,
  p.measurement_year,
  c.proportion_basis,
  c.condition_proportion_unadjusted,
  pop.expansion_factor,
  pop.adjustment_factor_for_the_subplot,
  pop.adjustment_factor_for_the_macroplot,
  CASE
    WHEN c.proportion_basis = 'SUBP'
      AND pop.adjustment_factor_for_the_subplot IS NOT NULL
      AND pop.adjustment_factor_for_the_subplot > 0
      AND pop.expansion_factor IS NOT NULL
      AND pop.expansion_factor > 0
      AND c.condition_proportion_unadjusted IS NOT NULL
    THEN pop.expansion_factor * c.condition_proportion_unadjusted * pop.adjustment_factor_for_the_subplot
    ELSE NULL
  END AS subplot_area,
  CASE
    WHEN c.proportion_basis = 'MACR'
      AND pop.adjustment_factor_for_the_macroplot IS NOT NULL
      AND pop.adjustment_factor_for_the_macroplot > 0
      AND pop.expansion_factor IS NOT NULL
      AND pop.expansion_factor > 0
      AND c.condition_proportion_unadjusted IS NOT NULL
    THEN pop.expansion_factor * c.condition_proportion_unadjusted * pop.adjustment_factor_for_the_macroplot
    ELSE NULL
  END AS macroplot_area
