WITH qualified_patents AS (
    SELECT DISTINCT p."id", p."title", p."abstract", p."date" AS "publication_date"
    FROM PATENTSVIEW.PATENTSVIEW.PATENT p
    INNER JOIN PATENTSVIEW.PATENTSVIEW.CPC_CURRENT c ON p."id" = c."patent_id"
    WHERE p."date" BETWEEN '2014-01-01' AND '2014-01-31'
      AND p."country" = 'US'
      AND (
           (c."subsection_id" BETWEEN 'C05' AND 'C13')
           OR c."group_id" IN ('A01G', 'A01H', 'A61K', 'A61P', 'A61Q',
                               'B01F', 'B01J', 'B81B', 'B82B', 'B82Y',
                               'G01N', 'G16H')
          )
)
SELECT qp."title", qp."abstract", qp."publication_date",
       COALESCE(bc."backward_citation_count", 0) AS "backward_citation_count",
       COALESCE(fc."forward_citation_count", 0) AS "forward_citation_count"
FROM qualified_patents qp
LEFT JOIN (
    SELECT b."patent_id", COUNT(DISTINCT b."citation_id") AS "backward_citation_count"
    FROM PATENTSVIEW.PATENTSVIEW.USPATENTCITATION b
    JOIN PATENTSVIEW.PATENTSVIEW.PATENT bp ON b."citation_id" = bp."id"
    JOIN qualified_patents qp2 ON b."patent_id" = qp2."id"
    WHERE bp."date" BETWEEN DATEADD(year, -5, qp2."publication_date") AND qp2."publication_date"
    GROUP BY b."patent_id"
) AS bc ON qp."id" = bc."patent_id"
LEFT JOIN (
    SELECT f."citation_id", COUNT(DISTINCT f."patent_id") AS "forward_citation_count"
    FROM PATENTSVIEW.PATENTSVIEW.USPATENTCITATION f
    JOIN PATENTSVIEW.PATENTSVIEW.PATENT fp ON f."patent_id" = fp."id"
    JOIN qualified_patents qp2 ON f."citation_id" = qp2."id"
    WHERE fp."date" BETWEEN qp2."publication_date" AND DATEADD(year, 5, qp2."publication_date")
    GROUP BY f."citation_id"
) AS fc ON qp."id" = fc."citation_id";
