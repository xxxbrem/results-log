Query:
SELECT p."player_name", bd."bowler", bd."balls_bowled", wd."total_wickets",
        (bd."balls_bowled" / 6.0) AS "overs_bowled",
        (rd."total_runs_conceded" / (bd."balls_bowled" / 6.0)) AS "economy_rate",
        (bd."balls_bowled" / wd."total_wickets") AS "strike_rate"
    FROM
        (SELECT "bowler", COUNT(*) AS "balls_bowled"
         FROM IPL.IPL.BALL_BY_BALL
         GROUP BY "bowler") bd
    JOIN
        (SELECT bb."bowler", COUNT(*) AS "total_wickets"
         FROM IPL.IPL.WICKET_TAKEN wt
         JOIN IPL.IPL.BALL_BY_BALL bb ON wt."match_id" = bb."match_id"
             AND wt."over_id" = bb."over_id"
             AND wt."ball_id" = bb."ball_id"
         GROUP BY bb."bowler") wd
    ON bd."bowler" = wd."bowler"
    JOIN
        (SELECT bb."bowler", SUM(bs."runs_scored") AS "total_runs_conceded"
         FROM IPL.IPL.BALL_BY_BALL bb
         JOIN IPL.IPL.BATSMAN_SCORED bs ON bb."match_id" = bs."match_id"
