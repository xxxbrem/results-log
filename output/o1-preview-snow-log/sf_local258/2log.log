Query:
SELECT p."player_name", wd."bowler", wd."match_id", wd."wickets_in_match", rd."runs_conceded",
        CONCAT(wd."wickets_in_match", '-', rd."runs_conceded") AS "best_performance"
    FROM
        (SELECT bb."bowler", wt."match_id", COUNT(*) AS "wickets_in_match"
         FROM IPL.IPL.WICKET_TAKEN wt
         JOIN IPL.IPL.BALL_BY_BALL bb ON wt."match_id" = bb."match_id"
             AND wt."over_id" = bb."over_id"
             AND wt."ball_id" = bb."ball_id"
         GROUP BY bb."bowler", wt."match_id") wd
    JOIN
        (SELECT bb."bowler", bb."match_id", SUM(bs."runs_scored") AS "runs_conceded"
         FROM IPL.IPL.BALL_BY_BALL bb
         JOIN IPL.IPL.BATSMAN_SCORED bs ON bb."match_id" = bs."match_id"
             AND bb."over_id" = bs."over_id"
             AND bb."ball_id" = bs."ball_id"
         GROUP BY bb."bowler", bb."match_id") rd
    ON wd."bowler" = rd."bowler" AND wd."match_id" = rd."match_id"
    JOIN IPL.IPL.PLAYER p ON wd."bowler" = p."player_id"
    ORDER BY wd."bowler", wd."wickets_in_match" DESC
    LIMIT 100;
