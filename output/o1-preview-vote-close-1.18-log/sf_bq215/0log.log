Query:
WITH cited_patents AS (
       -- same as above
   ),
   cited_ipc_codes AS (
       -- same as above
   ),
   ipc_class_counts AS (
       SELECT 
           citing_pub_num, 
           cited_ipc4_code, 
           COUNT(*) AS n_ij 
       FROM 
           cited_ipc_codes 
       GROUP BY 
           citing_pub_num, cited_ipc4_code
   ),
   total_citations AS (
       SELECT 
           citing_pub_num, 
           SUM(n_ij) AS N_i 
       FROM 
           ipc_class_counts 
       GROUP BY 
           citing_pub_num
   ),
   originality_scores AS (
       SELECT 
           i.citing_pub_num, 
           1 - SUM(POWER((i.n_ij / t.N_i), 2)) AS originality_score 
       FROM 
           ipc_class_counts i 
       JOIN 
           total_citations t 
           ON i.citing_pub_num = t.citing_pub_num 
       GROUP BY 
           i.citing_pub_num
   )
   SELECT 
       citing_pub_num AS publication_number, 
       originality_score 
   FROM 
       originality_scores 
   ORDER BY 
       originality_score DESC 
   LIMIT 1;
